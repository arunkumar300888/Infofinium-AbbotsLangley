<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.co.jmr.sdp.domain">
<class name="Doctype" table="doc_type">
        <id name="id" unsaved-value="-1">
            <generator class="native"/>
        </id>
        <property name="doctypeName" column="doctype_name"/>
        <property name="description" column="description"/>
        <property name="workflow" column="isWorkflow" type="boolean"></property>
        <property name="abbreviation" column="abbreviation" type="string"></property>
        <property name="isActive" column="isActive" type="character"></property>
        <property name="version" column="isVersion" type="string"></property>
</class>
</hibernate-mapping>

