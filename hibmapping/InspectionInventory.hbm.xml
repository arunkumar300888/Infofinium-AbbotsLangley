<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.co.jmr.sdp.domain">
	<class name="InspectionInventory" table="inspection_inventory">

		<id name="id" unsaved-value="-1"  >
			<generator class="native"></generator>
		</id>
		<property name="userFormId" column="user_form_id" />
		<property name="tenantName" column="tenantname" />
		<property name="employeeName" column="employeename" />
		<property name="address" column="address" />
		<property name="room" column="room" />
		<property name="furnishingsList" column="listoffurnishings" />
		<property name="ok" column="everythingok" />
		<property name="notes" column="notes" />
		<property name="photos" column="photos" />
		<property name="propertyForm" column="propertyform" />
		<property name="tenancyForm" column="tenancyform" />
	</class>
</hibernate-mapping>
